{
  "name": "@autocode2/cli",
  "version": "0.0.1",
  "description": "Autocode CLI",
  "author": "Gareth Andrew <gingerhendrix@gmail.com>",
  "license": "MIT",
  "keywords": [
    "llm",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/autocode2/autocode2.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7.5.1"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:build": "rimraf ./dist",
    "dev": "nodemon",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@autocode2/core": "workspace:*",
    "@autocode2/ink": "workspace:*",
    "@inquirer/core": "^9.2.1",
    "@inquirer/prompts": "^6.0.1",
    "@inquirer/type": "^1.5.5",
    "@oclif/core": "^4.0.17",
    "ansi-escapes": "^7.0.0",
    "chalk": "^5.3.0",
    "external-editor": "^3.1.0",
    "tiny-invariant": "^1.3.3",
    "tty-table": "^4.2.3"
  },
  "bin": {
    "auto-code2": "./bin/run.js"
  },
  "oclif": {
    "bin": "auto-code-cli",
    "commands": "./dist/commands",
    "dirname": "auto-code-cli",
    "topicSeparator": ":"
  },
  "devDependencies": {
    "@types/node": "^18",
    "ts-node": "^10.9.2"
  }
}
