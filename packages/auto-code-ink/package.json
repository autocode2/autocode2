{
  "name": "@autocode2/ink",
  "version": "0.2.2",
  "description": "Autocode console UI",
  "author": "Gareth Andrew <gingerhendrix@gmail.com>",
  "license": "MIT",
  "keywords": [
    "llm",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/autocode2/autocode2.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7.5.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:build": "rimraf ./dist",
    "dev": "nodemon",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@autocode2/core": "*",
    "@inkjs/ui": "^2.0.0",
    "chalk": "^5.3.0",
    "external-editor": "^3.1.0",
    "ink": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/uuid": "^10.0.0",
    "prettier": "^3.3.3",
    "type-fest": "^4.26.1"
  }
}
